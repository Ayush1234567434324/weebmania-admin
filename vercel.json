{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "admin/build",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/admin/(.*)",
      "dest": "admin/build/$1"
    },
    {
      "src": "^/admin$",
      "dest": "admin/build/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "src/server.js"
    }
  ]
}
