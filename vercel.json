{
  "version": 2,
  "builds": [
    {
      "src": "admin/package.json", 
      "use": "@vercel/static-build"
    },
    {
      "src": "src/server.js", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/admin/(.*)",
      "dest": "/admin/build/index.html"
    }
    ,
    {
      "src": "/(.*)",
      "dest": "src/server.js"
    }
  ]
}
