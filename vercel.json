{
  "version": 2,
  "builds": [
    {
      "src": "admin/package.json", 
      "use": "@vercel/static-build",
      "buildCommand": "npm run build"

      
    },
    {
      "src": "src/server.js", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.js"
    }
  ]
}
